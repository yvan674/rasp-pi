<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.shape.Line?>
<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="elite.EliteController"
            prefHeight="320.0" prefWidth="480.0"
            styleClass="root">
    <!-- the borders for the design -->
    <Line styleClass="borders"
          strokeWidth="4"
          stroke="#f39200"
          startX="20"
          startY="52"
          endX="460"
          endY="52"/>
    <Line styleClass="borders"
          strokeWidth="4"
          stroke="#f39200"
          startX="20"
          startY="268"
          endX="460"
          endY="268"/>
    <Line strokeWidth="2"
          stroke="#f8b205"
          startX="20"
          startY="52"
          endX="460"
          endY="52"/>
    <Line strokeWidth="2"
          stroke="#f8b205"
          startX="20"
          startY="268"
          endX="460"
          endY="268"/>
    <!-- end of the borders -->

    <!-- the close button -->
    <Button fx:id="closeButton"
            id="close-button"
            onAction="#closeButtonPressed"
            layoutX="425"
            layoutY="8"/>
    <!-- end of the close button -->

    <!-- all the buttons -->
    <Button fx:id="superCruiseButton"
            id="superCruise-button"
            styleClass="button"
            layoutX="51"
            layoutY="72"
            onAction="#superCruisePressed"/>
    <Button fx:id="mapButton"
            id="map-button"
            styleClass="button"
            layoutX="150"
            layoutY="71"
            onAction="#mapPressed"/>
    <Button fx:id="universalButton"
            id="universal-button"
            styleClass="button"
            layoutX="263"
            layoutY="69"
            onAction="#universalPressed"/>
    <Button fx:id="abortButton"
            id="abort-button"
            styleClass="button"
            layoutX="369"
            layoutY="71"
            onAction="#abortPressed"/>
    <Button fx:id="fsdButton"
            id="fsd-button"
            styleClass="button"
            layoutX="41"
            layoutY="182"
            onAction="#fsdPressed"/>
    <Button fx:id="systemButton"
            id="fsd-button"
            styleClass="button"
            layoutX="149"
            layoutY="178"
            onAction="#systemPressed"/>
    <Button fx:id="dockButton"
            id="dock-button"
            styleClass="button"
            layoutX="260"
            layoutY="181"
            onAction="#dockPressed"/>
    <Button fx:id="repairButton"
            id="repair-button"
            styleClass="button"
            layoutX="371"
            layoutY="181"
            onAction="#repairPressed"/>
    <!-- end of all the buttons -->

    <stylesheets>
        <URL value="@elite.css"/>
    </stylesheets>
</AnchorPane>
